     //SUM OF DIGITS
/*
JAYANT MITTAL
*/
#include<bits/stdc++.h>
using namespace std;
#define speed_up ios_base::sync_with_stdio(false);cin.tie(NULL);
#define f(i,n) for(i=0;i<n;i++)
typedef long long int lli;
int main()
{
    lli i,sum,c=0;
    string str;
    cin>>str;
    while(str.length()!=1)
    { sum=0;
        for(i=0;i<str.length();i++)
        {
            sum+=str[i]-48;
            
        }
        str=to_string(sum);
        c++;
    }
    cout<<c;
}
